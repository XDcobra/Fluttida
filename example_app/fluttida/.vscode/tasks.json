{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter: Run (auto version)",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        "${workspaceFolder}/example_app/fluttida/tools/flutter_run_with_version.ps1"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/example_app/fluttida"
      }
    }
  ]
}
